<Window x:Class="plotSinCos.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:ScottPlot="clr-namespace:ScottPlot.WPF;assembly=ScottPlot.WPF"
        xmlns:local="clr-namespace:plotSinCos"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
       
        <TabControl>
            <TabItem Header="Graph1">
                <ScottPlot:WpfPlot x:Name="WpfPlot1" />
            </TabItem>
            <TabItem Header="Graph2">
                <ScottPlot:WpfPlot x:Name="WpfPlot2" />
            </TabItem>
            <TabItem Header="Graph3">
                <ScottPlot:WpfPlot x:Name="WpfPlot3" />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
